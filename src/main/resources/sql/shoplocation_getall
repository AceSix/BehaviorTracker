select a.shopid , b.shopname , a.latitude , a.longitude  FROM shoplocation a inner join shopinfo b
on a.shopid = b.shopid
where a.shopid IN ("10011311","58480512","5215576","23608199","5143587","77323241","17183882","5505589","6261849","79211010","74593836","69841407","14719447","75173207","32320509","14716515","66888222","22405871","69717321","75187452","50393771","19326490","18594623","67021639")
group by a.shopid